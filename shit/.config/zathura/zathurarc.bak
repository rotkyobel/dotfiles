### Settings from original zathurarc ###
set sandbox none

### zathura-pywal ###
# Allow recolor
set recolor "true"

# Don't allow original hue when recoloring
set recolor-keephue "false"

# Don't keep original image colors while recoloring
set recolor-reverse-video "false"

# Command line completion entries
set completion-fg "#cbcce9"
set completion-bg "#0b0d11"

# Command line completion group elements
set completion-group-fg "#575166"
set completion-group-bg "#0b0d11"

# Current command line completion element
set completion-highlight-fg "#0b0d11"
set completion-highlight-bg "#cbcce9"

# Default foreground/background color
set default-bg rgba(11,13,17,1)

# Input bar
set inputbar-fg "#cbcce9"
set inputbar-bg "#0b0d11"

# Notification
set notification-fg "#cbcce9"
set notification-bg "#0b0d11"

# Error notification
set notification-error-fg "#cbcce9"
set notification-error-bg "#3A4456"

# Warning notification
set notification-warning-fg "#cbcce9"
set notification-warning-bg "#3A4456"

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg "#cbcce9"
set statusbar-bg "#0b0d11"

# Highlighting parts of the document (e.g. show search results)
set highlight-color "#575166"
set highlight-active-color "#575166"

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor "#cbcce9"

# "Loading..." text
set render-loading-fg "#cbcce9"
set render-loading-bg "#0b0d11"

# Index mode
set index-fg "#cbcce9"
set index-bg "#0b0d11"

# Selected element in index mode
set index-active-fg "#0b0d11"
set index-active-bg "#cbcce9"